    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "less-middleware": "~2.0.1",
    "react-dom": "~0.14.3",
    "react": "~0.14.3",
    "socket.io": "~1.3.7",
    "babelify": "~7.2.0",
    "babel-preset-react": "~6.3.13",
    "jquery": "~2.1.4",
    "moment": "~2.10.6"
  }
}
